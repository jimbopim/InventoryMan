<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jimla.birthdayreminder">
    <uses-feature android:name="android.hardware.camera" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BirthdayReminder">

        <activity
            android:name=".ProjectActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jimla.birthdayreminder.RoomActivity">
        </activity>
        <activity
            android:name="com.jimla.birthdayreminder.RoomDetails">
        </activity>
        <activity
            android:name="com.jimla.birthdayreminder.ItemDetails">
        </activity>
        <activity
            android:name="com.jimla.birthdayreminder.InventoryActivity">
        </activity>
        <activity
            android:name="com.jimla.birthdayreminder.ProjectDetails">
        </activity>

    </application>

</manifest>